@model CategoryModel
@using PlantOasis.lib.Models


<div class="category-item">
    <a href="@Model.Url">
        <div class="img">
            <img src="@Model.AdminImgUrl" alt="@Model.CategoryName" />
        </div>
        <div class="title">
            @Model.CategoryName
        </div>
    </a>
</div>
